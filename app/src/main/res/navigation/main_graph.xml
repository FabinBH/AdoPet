<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.projetopi.ui.SplashFragment"
        android:label="SplashFragment"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_authentication"
            app:destination="@id/authentication"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <navigation android:id="@+id/authentication"
        app:startDestination="@id/dashboardFragment">
        <fragment
            android:id="@+id/dashboardFragment"
            android:name="com.example.projetopi.ui.DashboardFragment"
            android:label="DashboardFragment"
            tools:layout="@layout/fragment_dashboard">
            <action
                android:id="@+id/action_dashboardFragment_to_loginFragment3"
                app:destination="@id/loginFragment3" />
            <action
                android:id="@+id/action_dashboardFragment_to_signupFragment2"
                app:destination="@id/signupFragment2" />
        </fragment>
        <fragment
            android:id="@+id/loginFragment3"
            android:name="com.example.projetopi.ui.auth.LoginFragment"
            android:label="LoginFragment"
            tools:layout="@layout/fragment_login">
            <action
                android:id="@+id/action_loginFragment3_to_recoverPasswordFragment"
                app:destination="@id/recoverPasswordFragment" />
        </fragment>
        <fragment
            android:id="@+id/signupFragment2"
            android:name="com.example.projetopi.ui.auth.SignupFragment"
            android:label="SignupFragment"
            tools:layout="@layout/fragment_signup">
            <action
                android:id="@+id/action_signupFragment2_to_locateFragment"
                app:destination="@id/locateFragment" />
        </fragment>
        <fragment
            android:id="@+id/recoverPasswordFragment"
            android:name="com.example.projetopi.ui.auth.RecoverPasswordFragment"
            android:label="RecoverPasswordFragment"
            tools:layout="@layout/fragment_recover_password"/>
        <action
            android:id="@+id/action_global_adoptionFragment"
            app:destination="@id/adoptionFragment"
            app:popUpTo="@id/loginFragment3"
            app:popUpToInclusive="true" />
        <fragment
            android:id="@+id/ongInfoFragment"
            android:name="com.example.projetopi.ui.auth.OngInfoFragment"
            android:label="fragment_ong_info"
            tools:layout="@layout/fragment_ong_info" >
            <action
                android:id="@+id/action_ongInfoFragment_to_ongInfo2Fragment"
                app:destination="@id/ongInfo2Fragment" />
        </fragment>
        <fragment
            android:id="@+id/ongInfo2Fragment"
            android:name="com.example.projetopi.ui.auth.OngInfo2Fragment"
            android:label="fragment_ong_info2"
            tools:layout="@layout/fragment_ong_info2" />
        <fragment
            android:id="@+id/personInfoFragment"
            android:name="com.example.projetopi.ui.auth.PersonInfoFragment"
            android:label="fragment_person_info"
            tools:layout="@layout/fragment_person_info" />
        <fragment
            android:id="@+id/locateFragment"
            android:name="com.example.projetopi.ui.auth.LocateFragment"
            android:label="fragment_locate"
            tools:layout="@layout/fragment_locate" >
            <action
                android:id="@+id/action_locateFragment_to_choosePersonFragment"
                app:destination="@id/choosePersonFragment" />
        </fragment>
        <fragment
            android:id="@+id/choosePersonFragment"
            android:name="com.example.projetopi.ui.auth.ChoosePersonFragment"
            android:label="fragment_choose_person"
            tools:layout="@layout/fragment_choose_person" >
            <action
                android:id="@+id/action_choosePersonFragment_to_ongInfoFragment"
                app:destination="@id/ongInfoFragment" />
            <action
                android:id="@+id/action_choosePersonFragment_to_personInfoFragment"
                app:destination="@id/personInfoFragment" />
        </fragment>
    </navigation>
    <fragment
        android:id="@+id/adoptionFragment"
        android:name="com.example.projetopi.ui.AdoptionFragment"
        android:label="AdoptionFragment"
        tools:layout="@layout/fragment_adoption">
        <action
            android:id="@+id/action_adoptionFragment_to_filterFragment"
            app:destination="@id/filterFragment" />
        <action
            android:id="@+id/action_adoptionFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_adoptionFragment_to_animalDetailsFragment"
            app:destination="@id/animalDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/adoptionConfirmationFragment"
        android:name="com.example.projetopi.ui.AdoptionConfirmationFragment"
        android:label="AdoptionConfirmationFragment"
        tools:layout="@layout/fragment_adoption_confirmation"/>
    <fragment
        android:id="@+id/ongFragment"
        android:name="com.example.projetopi.ui.OngFragment"
        android:label="OngFragment"
        tools:layout="@layout/fragment_ong">
        <action
            android:id="@+id/action_ongFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
        <action
            android:id="@+id/action_ongFragment_to_ongDetailsFragment"
            app:destination="@id/ongDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/animalRegisterFragment"
        android:name="com.example.projetopi.ui.AnimalRegisterFragment"
        android:label="AnimalRegisterFragment"
        tools:layout="@layout/fragment_animal_register"/>
    <fragment
        android:id="@+id/filterFragment"
        android:name="com.example.projetopi.ui.util.FilterFragment"
        android:label="FilterFragment"
        tools:layout="@layout/fragment_filter"/>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.projetopi.ui.RegisterFragment"
        android:label="RegisterFragment"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_registerFragment_to_animalRegisterFragment"
            app:destination="@id/animalRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.projetopi.ui.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_authentication2"
            app:destination="@id/authentication" />
    </fragment>
    <fragment
        android:id="@+id/socialFragment"
        android:name="com.example.projetopi.ui.SocialFragment"
        android:label="fragment_social"
        tools:layout="@layout/fragment_social" />
    <fragment
        android:id="@+id/animalDetailsFragment"
        android:name="com.example.projetopi.ui.AnimalDetailsFragment"
        android:label="AnimalDetailsFragment"
        tools:layout="@layout/fragment_animal_details">
        <action
            android:id="@+id/action_animalDetailsFragment_to_adoptionConfirmationFragment"
            app:destination="@id/adoptionConfirmationFragment" />
    </fragment>
    <fragment
        android:id="@+id/ongDetailsFragment"
        android:name="com.example.projetopi.ui.OngDetailsFragment"
        android:label="OngDetailsFragment"
        tools:layout="@layout/fragment_ong_details">
        <action
            android:id="@+id/action_ongDetailsFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/ongConfirmationFragment"
        android:name="com.example.projetopi.ui.OngConfirmationFragment"
        android:label="fragment_ong_confirmation"
        tools:layout="@layout/fragment_ong_confirmation" >
        <action
            android:id="@+id/action_ongConfirmationFragment_to_ongFragment"
            app:destination="@id/ongFragment" />
    </fragment>
</navigation>